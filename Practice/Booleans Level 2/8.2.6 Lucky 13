public int luckyThirteen(int x, int y)
{
    if(x < 13 && y > 13)
    {
        return x;
    }
    else if(x > 13 && y < 13)
    {
        return y;
    }
    else if(x < 13 && y <= 13 && y > x)
    {
        return y;
    }
    else if(x < 13 && y < 13 && y < x)
    {
        return x;
    }
    else if(x > 13 && y > 13)
    {
        return 0;
    }
    else
    {
        return x;
    }
}
