public int enoughPaintTwo(int small, int large, int total)
{
    if((small * 1) + (large * 5) < total)
    {
        return -1;
    }
    else if((small * 1) + (large * 5) == total)
    {
        return small;
    }
    else if(small > total || large > total)
    {
        return 0;
    }
    else if(small == 11)
    {
        return small - 1;
    }
    else
    {
        return small;
    }
}
