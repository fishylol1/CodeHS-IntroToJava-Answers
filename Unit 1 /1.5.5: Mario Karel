public class MarioKarel extends Karel
{
    public void run()
    {
        turnLeft();
        move();
        move();
        move();
        turnRight();
        move();
        collectCoin();
        turnLeft();
        move();
        turnRight();
        move();
        move();
        turnRight();
        move();
        collectCoin();
        move();
        turnLeft();
        move();
        move();
        turnLeft();
        move();
        collectCoin();
        move();
        turnRight();
        move();
        move();
        turnRight();
        move();
        collectCoin();
        move();
        move();
        move();
        turnLeft();
    }
    
    public void turnRight()
    {
        turnLeft();
        turnLeft();
        turnLeft();
    }
    
    public void collectCoin()
    {
        takeBall();
        takeBall();
    }
}
