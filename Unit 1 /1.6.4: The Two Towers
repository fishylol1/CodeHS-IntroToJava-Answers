public class TwoTowersKarel extends Karel
{

	public void run()
	{
		move();
		buildTower();
		turnRight();
		move();
		turnRight();
		move();
		move();
		turnLeft();
		move();
		buildTower();
		move();
		turnRight();
	}
	
	public void turnRight()
	{
	    turnLeft();
	    turnLeft();
	    turnLeft();
	}
	
    public void buildTower()
    {
        putBall();
        turnLeft();
        move();
        putBall();
        move();
        putBall();
    }

}
