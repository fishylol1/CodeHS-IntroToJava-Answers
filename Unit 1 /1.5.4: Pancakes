public class PancakeKarel extends Karel
{
    public void run()
    {
        move();
        makePancakes();
        move();
        move();
        makePancakes();
        move();
        move();
        makePancakes();
        move();
    }
    
    public void makePancakes()
    {
        putBall();
        putBall();
        putBall();
    }
}
